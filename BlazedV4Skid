                                                                                if Lock.Visualize.Tracer[1] then 
                                                                                    Lock.Drawings.Tracer.Visible = true 
                                                                                    --
                                                                                    if Lock:GetTracerOrigin(Lock.Visualize.Tracer[2]) ~= nil then 
                                                                                        local Position2, OnScreen = Lock:GetTracerOrigin(Lock.Visualize.Tracer[2])  
