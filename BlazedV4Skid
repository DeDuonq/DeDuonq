                wait()
                inputManager:SendKeyEvent(true, "O", false, game)
                wait()
                inputManager:SendKeyEvent(true, "I", false, game)
                wait()
                inputManager:SendKeyEvent(true, "O", false, game)
                wait()
                inputManager:SendKeyEvent(true, "I", false, game)
                wait()
                inputManager:SendKeyEvent(true, "O", false, game)
                wait()
            until getgenv().mawco == false
    end
    end
