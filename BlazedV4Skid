        Lock.Drawings.FakeHitbox.CanCollide = false 
        Lock.Drawings.FakeHitbox.CFrame = CFrame.new(9999,9999,9999)
        Lock.Drawings.FakeHitbox.Parent = game.Workspace
        Lock.Drawings.FakeHitbox.Material = Enum.Material.Neon
