ModePS:Toggle("Loot Aura", function(val)
LootAura = val
if LootAura then
        local function setup(room)
            local function check(v)
                if v:IsA("Model") then
                    if v.Name == "DrawerContainer" then
                        local knob = v:WaitForChild("Knobs")
                        
                        if knob then
                            local prompt = knob:WaitForChild("ActivateEventPrompt")
                            local interactions = prompt:GetAttribute("Interactions")
                            
                            if not interactions then
                                task.spawn(function()
                                    repeat task.wait(0.1)
                                        if lp:DistanceFromCharacter(knob.Position) <= 12 then
                                            fireproximityprompt(prompt)
                                        end
                                    until prompt:GetAttribute("Interactions") or not LootAura
                                end)
                            end
                        end
                    elseif v.Name == "GoldPile" then
                        local prompt = v:WaitForChild("LootPrompt")
                        local interactions = prompt:GetAttribute("Interactions")
                            
                        if not interactions then
                            task.spawn(function()
                                repeat task.wait(0.1)
                                    if lp:DistanceFromCharacter(v.PrimaryPart.Position) <= 12 then
                                        fireproximityprompt(prompt) 
                                    end
                                until prompt:GetAttribute("Interactions") or not LootAura
                            end)
                        end
                    elseif v.Name:sub(1,8) == "ChestBox" then
                        local prompt = v:WaitForChild("ActivateEventPrompt")
                        local interactions = prompt:GetAttribute("Interactions")
                        
                        if not interactions then
                            task.spawn(function()
                                repeat task.wait(0.1)
                                    if lp:DistanceFromCharacter(v.PrimaryPart.Position) <= 12 then
                                        fireproximityprompt(prompt)
                                    end
                                until prompt:GetAttribute("Interactions") or not LootAura
                            end)
                        end
                    elseif v.Name == "RolltopContainer" then
                        local prompt = v:WaitForChild("ActivateEventPrompt")
                        local interactions = prompt:GetAttribute("Interactions")
                        
                        if not interactions then
                            task.spawn(function()
                                repeat task.wait(0.1)
                                    if lp:DistanceFromCharacter(v.PrimaryPart.Position) <= 12 then
                                        fireproximityprompt(prompt)
                                    end
                                until prompt:GetAttribute("Interactions") or not LootAura
                            end)
                        end
                    end 
                end
            end
    
            local subaddcon
            subaddcon = room.DescendantAdded:Connect(function(v)
                check(v) 
            end)
            
            for i,v in pairs(room:GetDescendants()) do
                check(v)
            end
            
            task.spawn(function()
                repeat task.wait() until not LootAura
                subaddcon:Disconnect() 
            end)
        end
        
        local addconnect
        addconnect = game.Workspace.CurrentRooms.ChildAdded:Connect(function(room)
            setup(room)
        end)
        
        for i,room in pairs(game.Workspace.CurrentRooms:GetChildren()) do
            if room:FindFirstChild("Assets") then
                setup(room) 
            end
        end
        
        repeat task.wait() until not LootAura
        addconnect:Disconnect()
        end
end)

ModePS:Label("Room Section")

ModePS:Button("Skip Room", function()
local room = game.Workspace.CurrentRooms[game:GetService("ReplicatedStorage").GameData.LatestRoom.Value]
local assets = room:WaitForChild("Assets")
local HaveKey = nil
for _,Key in pairs(assets:GetChildren()) do
if Key.Name == "KeyObtain" then
HaveKey = Key
end
end
if HaveKey then
lp.Character.Humanoid:PivotTo(HaveKey.Hitbox:GetPivot())
wait(0.4)
fireproximityprompt(HaveKey.ModulePrompt,0)
wait(0.6)
lp.Character.Humanoid:PivotTo(room:WaitForChild("Door"):WaitForChild("Door"):GetPivot())
wait(0.5)
fireproximityprompt(room:WaitForChild("Door"):WaitForChild("Door").Lock.UnlockPrompt,0)
room = nil
else
lp.Character.Humanoid:PivotTo(room:WaitForChild("Door"):WaitForChild("Door").GetPivot())
wait(0.5)
room:WaitForChild("Door"):WaitForChild("Door").ClientOpen:FireServer()
wait(0.1)
room = nil
end
end)

Remover:Toggle("Delete Gates", function(val)
    if val then
        local addconnect
        addconnect = workspace.CurrentRooms.ChildAdded:Connect(function(room)
            local gate = room:WaitForChild("Gate",2)
            
            if gate then
                local door = gate:WaitForChild("ThingToOpen",2)
                
                if door then
                    door:Destroy() 
                end
            end
        end)
 else
        addconnect:Disconnect()
    end
end)

