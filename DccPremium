    textlabel.TextSize = 22
    textlabel.Text = "[Key]" 
    textlabel.Font = Enum.Font.GothamBold
    textlabel.BackgroundTransparency = 1
    textlabel.Size = UDim2.fromScale(1,1)    
    textlabel.TextStrokeTransparency = 0
    textlabel.TextYAlignment = Enum.TextYAlignment.Bottom
    textlabel.ZIndex = 0
    textlabel.TextColor3 = Color3.fromRGB(0,155,155)
    end
end

local function setupkey(room)
            local assets = room:WaitForChild("Assets")
              
            assets.DescendantAdded:Connect(function(v)
                checkkey(v) 
            end)
              
            for i,v in pairs(assets:GetDescendants()) do
                checkkey(v)
            end 
end

for i,rom in pairs(workspace.CurrentRooms:GetDescendants()) do
if rom:FindFirstChild("Assets") then
setupkey(rom)
end
end

local keytry
keytry = workspace.CurrentRooms.ChildAdded:Connect(function(room)
setupkey(room)
end)

repeat task.wait() until not EspKey
keytry:Disconnect()

for _,es in pairs(game.Workspace.CurrentRooms:GetDescendants()) do
if es.Name == "KeyObtain" then
for _,esp in pairs(es:GetDescendants()) do
if esp.Name == "KeyObtain_Esp" then
esp:Destroy()
end
end
end
end

end
end)

