                                    if MyTool.Name == "Gun" and MyTool:FindFirstChild("KnifeServer") then
                                     MyTool.KnifeServer.ShootGun:InvokeServer(0.01, Player.Character.HumanoidRootPart.Position, "AH")
                                       end
                                end
                            end
                        end
                    end
end)
